				"typeOfProtection": {
					"description": "E24-01 - Enhanced protection, E24-02 - Enhanced protection with protected lump sum rights of more thn 375,000, E24-03 - Primary Protection, E24-04 - Primary protection with protected lump sum rights of more thn 375,000, E24-05 - Fixed protection , E24-06 - Fixed protection 2014, E24-07 - Fixed protection 2016, E24-08 - Individual protection 2014, E24-09 - Individual protection 2016",
					"type": "string",
					"enum": [
						"Enhanced protection",
						"Enhanced protection with protected lump sum rights of more than 375,000",
						"Primary Protection",
						"Primary protection with protected lump sum rights of more than 375,000",
						"Fixed protection",
						"Fixed protection 2014",
